{
  "name": "mezcalito/ux-filemanager",
  "type": "symfony-bundle",
  "description": "File manager for Symfony application",
  "keywords": [
    "symfony-ux"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.3",
    "symfony/dependency-injection": "^6.4|^7.0",
    "symfony/http-foundation": "^6.4|^7.0",
    "symfony/http-kernel": "^6.4|^7.0",
    "symfony/options-resolver": "^6.4|^7.0",
    "symfony/routing": "^6.4|^7.0",
    "symfony/ux-live-component": "^2.16",
    "twig/twig": "^3.0.4"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.41",
    "phpstan/phpstan": "^1.10",
    "phpstan/phpstan-symfony": "^1.3",
    "phpunit/phpunit": "^11.1",
    "rector/rector": "^0.19",
    "symfony/framework-bundle": "^6.4|^7.0",
    "symfony/twig-bundle": "^6.4|^7.0",
    "symfony/ux-twig-component": "^2.16"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Mezcalito\\FileManagerBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mezcalito\\FileManagerBundle\\Tests\\TestApplication\\": "tests/TestApplication/src",
      "Mezcalito\\FileManagerBundle\\Tests\\": "tests"
    },
    "files": [ "tests/helper-functions.php" ]
  }
}
