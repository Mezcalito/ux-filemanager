{
  "compilerOptions": {
    "lib": ["dom", "es2015"],
    "module": "es2015",
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "rootDir": "assets",
    "strict": true,
    "strictPropertyInitialization": false,
    "target": "es2017",
    "removeComments": true,
    "outDir": "assets/dist/types",
    "baseUrl": ".",
    "noEmit": false,
    "declaration": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  }
}